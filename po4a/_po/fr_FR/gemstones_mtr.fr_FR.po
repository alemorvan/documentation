#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Loss% - packet loss rate\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Snt - the number of packets sent\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Last - the delay of the last packet\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Avg - average delay\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Best - lowest latency\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "Wrst - Worst time delay\n"
msgstr ""

#. type: Bullet: '* '
#: ../src/docs/gemstones/mtr.md:62
#, no-wrap
msgid "StDev - variance (stability)\n"
msgstr ""

#. type: Title ##
#: ../src/docs/gemstones/mtr.md:63
#, no-wrap
msgid "Shortcut keys for interaction"
msgstr ""

#. type: Plain text
#: ../src/docs/gemstones/mtr.md:70
#, no-wrap
msgid ""
"<kbd>p</kbd> - pause;\n"
"<kbd>d</kbd> - switch display mode;\n"
"<kbd>n</kbd> - turn on/off DNS;\n"
"<kbd>r</kbd> - reset all counters;\n"
"<kbd>j</kbd> - Toggle delay display information;\n"
"<kbd>y</kbd> - switch IP information;\n"
"<kbd>q</kbd> - Quit interaction.\n"
msgstr ""
