#. type: Title ##
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:95
#, no-wrap
msgid "Demo"
msgstr ""

#. type: Plain text
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:98
#, no-wrap
msgid "**Our requirement is-Rocky Linux 8's /dir1/ directory is automatically synchronized to Fedora 34's /dir2/ directory; at the same time, Fedora 34's /dir2/ directory is automatically synchronized to Rocky Linux 8's /dir1/ directory**\n"
msgstr ""

#. type: Title ###
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:99
#, no-wrap
msgid "Configure Rcoky Linux 8"
msgstr ""

#. type: Fenced code block (bash)
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:101
#, no-wrap
msgid ""
"[root@Rocky ~]# mkdir /dir1\n"
"[root@Rocky ~]# setfacl -m u:testrsync:rwx /dir1/\n"
"[root@Rocky ~]# vim /root/unison1.sh\n"
"#!/bin/bash\n"
"a=\"/usr/local/inotify-tools/bin/inotifywait -mrq -e create,delete,modify,move /dir1/\"\n"
"b=\"/usr/local/bin/unison -batch /dir1/ ssh://testrsync@192.168.100.5//dir2\"\n"
"$a | while read directory event file\n"
"do\n"
"    $b &>> /tmp/unison1.log\n"
"done\n"
"[root@Rocky ~]# chmod +x /root/unison1.sh\n"
"[root@Rocky ~]# bash /root/unison1.sh &\n"
"[root@Rocky ~]# jobs -l\n"
msgstr ""

#. type: Title ###
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:117
#, no-wrap
msgid "Configure Fedora 34"
msgstr ""

#. type: Fenced code block (bash)
#: ../src/docs/books/learning_rsync/07_rsync_unison_use.md:119
#, no-wrap
msgid ""
"[root@fedora ~]# mkdir /dir2\n"
"[root@fedora ~]# setfacl -m u:testrsync:rwx /dir2/\n"
"[root@fedora ~]# vim /root/unison2.sh\n"
"#!/bin/bash\n"
"a=\"/usr/local/inotify-tools/bin/inotifywait -mrq -e create,delete,modify,move /dir2/\"\n"
"b=\"/usr/local/bin/unison -batch /dir2/ ssh://testrsync@192.168.100.4//dir1\"\n"
"$a | while read directory event file\n"
"do\n"
"    $b &>> /tmp/unison2.log\n"
"done\n"
"[root@fedora ~]# chmod +x /root/unison2.sh\n"
"[root@fedora ~]# bash /root/unison2.sh &\n"
"[root@fedora ~]# jobs -l\n"
msgstr ""

